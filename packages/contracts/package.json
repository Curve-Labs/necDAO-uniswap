  },
  "scripts": {
    "compile": "npx truffle compile",
    "ganache:start": "bash scripts/ganache.sh",
    "ganache:stop": "bash scripts/ganache.sh stop",
    "test": "bash scripts/ganache.sh test",
    "abi:copy": "mkdir -p node_modules/@daostack/migration/custom-abis && cp build/contracts/UniswapScheme.json node_modules/@daostack/migration/custom-abis/UniswapScheme.json && cp ./build/contracts/WETH9.json node_modules/@daostack/migration/custom-abis/WETH.json && cp ./build/contracts/UniswapV2Factory.json node_modules/@daostack/migration/custom-abis/UniswapV2Factory.json && cp build/contracts/UniswapV2Router02.json node_modules/@daostack/migration/custom-abis/UniswapV2Router.json",
    "migrate": "truffle exec scripts/deploy.js && node scripts/migrate.js"
  },
  "dependencies": {
    "@uniswap/v2-core": "^1.0.1",
    "@uniswap/v2-periphery": "^1.1.0-beta.0"
  },
  "devDependencies": {
    "@babel/plugin-transform-destructuring": "^7.10.4",
    "@daostack/arc": "0.0.1-rc.41",
    "@daostack/infra": "0.0.1-rc.17",
    "@daostack/migration": "0.0.1-rc.44-v0",
    "@openzeppelin/test-helpers": "^0.5.6",
    "babel-polyfill": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "babel-preset-stage-3": "^6.24.1",
    "babel-register": "^6.26.0",
    "chai": "^4.2.0",
    "dotenv": "^8.2.0",
    "ganache-cli": "6.10.0-beta.2",
    "js-sha3": "^0.8.0",
    "openzeppelin-solidity": "2.4.0",
    "truffle": "^5.1.34"
  }
}
